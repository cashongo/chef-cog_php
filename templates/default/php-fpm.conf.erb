include = <%= "#{node['peachy_frontend']['php-fpm']['pool_conf_dir']}/*.conf" %>


pid                             = <%= node['peachy_frontend']['php-fpm']['pid'] %>
error_log                       = <%= node['peachy_frontend']['php-fpm']['error_log'] %>
log_level                       = <%= node['peachy_frontend']['php-fpm']['log_level'] %>
emergency_restart_threshold     = 0
emergency_restart_interval      = 0
process_control_timeout         = <%= node['peachy_frontend']['php-fpm']['process_control_timeout'] %>
daemonize                       = yes
