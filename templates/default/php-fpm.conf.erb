; File maintained by chef
; MANUAL CHANGES WILL BE IGNORED

include = <%= @pool_config_directory %>/*.conf

[global]
pid = <%= @pid %>
error_log = <%= @error_log %>
log_level = <%= @log_level %>
daemonize = <%= @daemonize %>

<% @fpm_options.each do |name,value| -%>
<%= name.to_s + ' = ' + value.to_s %>
<% end -%>
